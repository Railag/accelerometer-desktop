<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.stage.Screen?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" prefWidth="${screen.visualBounds.width}"
            prefHeight="${screen.visualBounds.height}">
    <fx:define>
        <Screen fx:factory="getPrimary" fx:id="screen"/>
    </fx:define>
    <Button fx:id="button" layoutX="32.0" layoutY="321.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="259.0"/>
    <ImageView fx:id="firstImage" fitHeight="101.0" fitWidth="147.0" layoutX="14.0" layoutY="58.0" pickOnBounds="true"
               preserveRatio="true"/>
    <ImageView fx:id="secondImage" fitHeight="101.0" fitWidth="147.0" layoutX="161.0" layoutY="58.0" pickOnBounds="true"
               preserveRatio="true"/>
</AnchorPane>