<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.stage.Screen?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" prefWidth="${screen.visualBounds.width}"
            prefHeight="${screen.visualBounds.height}">
    <fx:define>
        <Screen fx:factory="getPrimary" fx:id="screen"/>
    </fx:define>
    <Button fx:id="focusingButton" layoutX="22.0" layoutY="149.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="259.0" text="Концентрация внимания" />
    <Button fx:id="distributionButton" layoutX="22.0" layoutY="187.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="259.0" text="Распределение внимания" />
    <Button fx:id="stabilityButton" layoutX="22.0" layoutY="227.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="259.0" text="Устойчивость внимания" />
    <Button fx:id="volumeButton" layoutX="22.0" layoutY="265.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="259.0" text="Объем внимания" />
   <Button fx:id="ramVolumeButton" layoutX="22.0" layoutY="307.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="259.0" text="Объем оперативной памяти" />
</AnchorPane>
